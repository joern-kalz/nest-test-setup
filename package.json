{
  "name": "@joern.kalz/nest-test-setup",
  "version": "0.1.2",
  "description": "Convenient test setup for Nest",
  "files": [
    "dist/**/*",
    "env.js"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc --project tsconfig-prod.json",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joern-kalz/nest-test-setup.git"
  },
  "keywords": [
    "nest",
    "test"
  ],
  "author": "joern.kalz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joern-kalz/nest-test-setup/issues"
  },
  "homepage": "https://github.com/joern-kalz/nest-test-setup#readme",
  "devDependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/testing": "^9.2.1",
    "@tsconfig/node10": "^1.0.9",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
